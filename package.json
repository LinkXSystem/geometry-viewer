{
  "name": "geometry-viewer",
  "productName": "Geometry Viewer",
  "description": "this is viewer",
  "version": "0.0.1",
  "private": true,
  "author": "zhi <linkxsystem@gmail.com>",
  "copyright": "Â© 2020, LinkXSystem inc.",
  "homepage": "https://github.com/LinkXSystem/geometry-viewer",
  "main": "app/app.js",
  "build": {
    "appId": "com.linksystem.geometry-viewer",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "test": "npm run unit",
    "release": "npm test && webpack --config=build/webpack.app.config.js --env=production && electron-builder",
    "start": "node build/start.js"
  },
  "dependencies": {
    "async": "^3.2.0",
    "fs-jetpack": "^2.1.0",
    "lodash": "^4.17.20",
    "three": "^0.122.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.2.1",
    "chai": "^4.1.0",
    "css-loader": "^3.1.0",
    "electron": "^8.2.3",
    "electron-builder": "^22.5.1",
    "electron-mocha": "^8.1.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mocha": "^7.1.1",
    "source-map-support": "^0.5.6",
    "spectron": "^10.0.1",
    "style-loader": "^1.1.4",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2"
  }
}
